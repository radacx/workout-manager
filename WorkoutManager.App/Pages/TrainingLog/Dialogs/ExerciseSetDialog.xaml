<Window x:Class="WorkoutManager.App.Pages.TrainingLog.Dialogs.ExerciseSetDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:controls="clr-namespace:WorkoutManager.App.Pages.TrainingLog.Controls"
        xmlns:models1="clr-namespace:WorkoutManager.App.Pages.TrainingLog.Models"
        xmlns:dialogs="clr-namespace:WorkoutManager.App.Pages.TrainingLog.Dialogs"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance Type=models1:ExerciseSetDialogViewModel}"
        Title="ExerciseSetDialog" Height="450" Width="800">
    <StackPanel>
        <controls:ExerciseSet DataContext="{Binding DataContext.ExerciseSet, RelativeSource={RelativeSource FindAncestor,
            AncestorType=dialogs:ExerciseSetDialog}}" />
        
        <Button
            Content="Submit"
            IsDefault="True"
            Click="Submit_OnClick"
        />
    </StackPanel>
</Window>
