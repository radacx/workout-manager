<UserControl x:Class="WorkoutManager.App.Pages.TrainingLog.Dialogs.ExerciseSetDialog"
        x:ClassModifier="internal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:models1="clr-namespace:WorkoutManager.App.Pages.TrainingLog.Models"
        xmlns:dialog="clr-namespace:WorkoutManager.App.Controls.Dialog"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance Type=models1:ExerciseSetDialogViewModel}"
>
    <UserControl.Resources>
        <models1:ExerciseSetDialogSelector x:Key="ExerciseSetDialogSelector" x:Shared="False" />
    </UserControl.Resources>
    
    <dialog:Dialog
        SubmitButtonTitle="{Binding SubmitButtonTitle}"
        DialogTitle="{Binding DialogTitle}"
    >
        <ContentControl Content="{Binding ExerciseSet}" ContentTemplateSelector="{StaticResource ExerciseSetDialogSelector}" />
    </dialog:Dialog>
</UserControl>
