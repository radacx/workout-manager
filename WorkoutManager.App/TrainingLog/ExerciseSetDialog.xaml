<Window x:Class="WorkoutManager.App.TrainingLog.ExerciseSetDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:models="clr-namespace:WorkoutManager.App.TrainingLog.Models"
        xmlns:trainingLog="clr-namespace:WorkoutManager.App.TrainingLog"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance Type=models:ExerciseSetDialogViewModel}"
        Title="ExerciseSetDialog" Height="450" Width="800">
    <StackPanel>
        <trainingLog:ExerciseSet DataContext="{Binding DataContext.ExerciseSet, RelativeSource={RelativeSource FindAncestor,
            AncestorType=trainingLog:ExerciseSetDialog}}" />
        
        <Button
            Content="Submit"
            IsDefault="True"
            Click="Submit_OnClick"
        />
    </StackPanel>
</Window>
